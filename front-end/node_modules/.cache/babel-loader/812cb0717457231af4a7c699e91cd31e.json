EAAA;AACA,gBAAA,KAAA,CAAA,IAAA,GAAA,EAAA;AACA,gBAAA,KAAA,CAAA,QAAA,GAAA,CAAA;AACA,gBAAA,KAAA,CAAA,OAAA,GAAA,CAAA;AACA,gBAAA,KAAA,CAAA,UAAA,GAAA,CAAA;AACA,gBAAA,KAAA,CAAA,YAAA,GAAA,CAAA;AACA,gBAAA,KAAA,CAAA,IAAA,GAAA,CAAA;AACA,gBAAA,KAAA,CAAA,QAAA,GAAA,KAAA;AApBA,iDAqBA,IArBA;;AAAA;AAAA;AAAA;AAuBA,gBAAA,OAAA,CAAA,GAAA;;AAvBA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AA0BA;AA9BA,GAfA;AA+CA,EAAA,UAAA,EAAA;AACA,IAAA,KAAA,EAAA;AADA,GA/CA;AAkDA,EAAA,OAlDA,qBAkDA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,qBAEA,KAAA,CAAA,GAAA,CAAA,YAAA,CAFA;;AAAA;AAEA,cAAA,QAFA;AAGA,cAAA,MAAA,CAAA,KAAA,CAAA,KAAA,CAAA,IAAA,GAAA,QAAA,CAAA,IAAA,CAAA,IAAA;AACA,cAAA,OAAA,CAAA,GAAA,CAAA,MAAA,CAAA,KAAA,CAAA,KAAA,CAAA,IAAA;AAJA;AAAA;;AAAA;AAAA;AAAA;AAMA,cAAA,MAAA,CAAA,KAAA,CAAA,KAAA,CAAA,IAAA,GAAA,IAAA;;AANA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAQA,GA1DA;AA2DA,EAAA,QAAA,EAAA;AACA,IAAA,IADA,kBACA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,KAAA,KAAA,CAAA,KAAA,CAAA,IAAA;AACA,aAAA,KAAA,KAAA,CAAA,KAAA,CAAA,IAAA;AACA;AAJA;AA3DA,CAAA","sourcesContent":["<template>\n<div>\n  <div v-if=\"user\">\n    <h1>Create an Adventurer:</h1>\n    <div class=\"form\">\n      <form v-if=\"creating\" @submit.prevent=\"addAdventurer\">\n        <label>Name:</label><br>\n        <input v-model=\"name\" placeholder=\"Name\">\n        <p></p>\n        <label>Race:</label><br>\n        <select v-model=\"race\" name=\"race\" id=\"race\">\n          <option value=\"Elf\">Elf</option>\n          <option value=\"Dwarf\">Dwarf</option>\n          <option value=\"Orc\">Orc</option>\n          <option value=\"Human\">Human</option>\n          <option value=\"Monster\">Monster</option>\n          <option value=\"Unknown\">Unknown</option>\n        </select>\n        <p></p>\n        <div class=\"stats\">\n          <div class=\"box\">\n            <label>Strength:</label>\n            <select v-model=\"strength\" name=\"strength\" id=\"strength\">\n              <option value=1>1</option><option value=2>2</option><option value=3>3</option><option value=4>4</option><option value=5>5</option><option value=6>6</option>\n              <option value=7>7</option><option value=8>8</option><option value=9>9</option><option value=10>10</option><option value=11>11</option><option value=12>12</option>\n            </select>\n          </div>\n          <div class=\"box\">\n            <label>Agility:</label>\n            <select v-model=\"agility\" name=\"agility\" id=\"agility\">\n              <option value=1>1</option><option value=2>2</option><option value=3>3</option><option value=4>4</option><option value=5>5</option><option value=6>6</option>\n              <option value=7>7</option><option value=8>8</option><option value=9>9</option><option value=10>10</option><option value=11>11</option><option value=12>12</option>\n            </select>\n          </div>\n          <div class=\"box\">\n            <label>Durability:</label>\n            <select v-model=\"durability\" name=\"durability\" id=\"durability\">\n              <option value=1>1</option><option value=2>2</option><option value=3>3</option><option value=4>4</option><option value=5>5</option><option value=6>6</option>\n              <option value=7>7</option><option value=8>8</option><option value=9>9</option><option value=10>10</option><option value=11>11</option><option value=12>12</option>\n            </select>\n          </div>\n          <div class=\"box\">\n            <label>Intelligence:</label>\n            <select v-model=\"intelligence\" name=\"intelligence\" id=\"intelligence\">\n              <option value=1>1</option><option value=2>2</option><option value=3>3</option><option value=4>4</option><option value=5>5</option><option value=6>6</option>\n              <option value=7>7</option><option value=8>8</option><option value=9>9</option><option value=10>10</option><option value=11>11</option><option value=12>12</option>\n            </select>\n          </div>\n        </div>\n        <p></p>\n        <label>Description:</label><br>\n        <textarea v-model=\"description\" placeholder=\"Description/Backstory\"></textarea>\n        <p></p>\n        <label>Cost of services: </label>\n        <input v-model=\"cost\" type=\"number\">\n        <label> gold</label>\n        <br />\n        <button type=\"submit\">Submit</button>\n      </form>\n      <div v-else>\n        <p>Adventurer created.</p>\n        <p><a @click=\"toggleForm\" href=\"#\">Click here to create a new one.</a></p>\n      </div>\n    </div>\n  </div>\n  <div v-else>\n    <Login/>\n  </div>\n</div>\n</template>\n\n<script>\nimport axios from 'axios';\nimport Login from '@/components/Login.vue';\nexport default {\n  name: 'create',\n  data() {\n    return {\n      creating: true,\n      name: '',\n      description: '',\n      race: '',\n      strength: 0,\n      agility: 0,\n      durability: 0,\n      intelligence: 0,\n      cost: 0\n    }\n  },\n  methods: {\n    toggleForm() {\n      this.creating = !this.creating;\n    },\n    async addAdventurer() {\n      try {\n        await axios.post(\"/api/adventurers\", {\n          name: this.name,\n          description: this.description,\n          race: this.race,\n          strength: this.strength,\n          agility: this.agility,\n          durability: this.durability,\n          intelligence: this.intelligence,\n          cost: this.cost\n        });\n        this.name = \"\";\n        this.description = \"\";\n        this.race = \"\";\n        this.strength = 0;\n        this.agility = 0;\n        this.durability = 0;\n        this.intelligence = 0;\n        this.cost = 0;\n        this.creating = false;\n        return true;\n      } catch (error) {\n        console.log(error);\n      }\n\n    },\n  },\n  components: {\n    Login,\n  },\n  async created() {\n    try {\n      let response = await axios.get('/api/users');\n      this.$root.$data.user = response.data.user;\n      console.log(this.$root.$data.user);\n    } catch (error) {\n      this.$root.$data.user = null;\n    }\n  },\n  computed: {\n    user() {\n      console.log(this.$root.$data.user);\n      return this.$root.$data.user;\n    }\n  },\n}\n</script>\n\n<style scoped>\ninput {\n  font-size: 1.2em;\n  height: 30px;\n  width: 200px;\n}\n\ntextarea {\n  font-size: 1.6em;\n  width: 100%;\n  max-width: 500px;\n  height: 100px;\n}\n\nbutton {\n  margin-top: 20px;\n  font-size: 1.2em;\n}\n</style>\n"],"sourceRoot":"src/views"}]}