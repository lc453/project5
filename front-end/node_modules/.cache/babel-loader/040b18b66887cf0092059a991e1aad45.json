AAAA;AAAA;AAAA;AAAA;AAEA,gBAAA,QAFA,GAEA,IAAA,QAAA,EAFA;AAGA,gBAAA,QAAA,CAAA,MAAA,CAAA,OAAA,EAAA,MAAA,CAAA,IAAA,EAAA,MAAA,CAAA,IAAA,CAAA,IAAA;AAHA;AAAA,uBAIA,KAAA,CAAA,IAAA,CAAA,aAAA,EAAA,QAAA,CAJA;;AAAA;AAIA,gBAAA,EAJA;AAAA;AAAA,uBAKA,KAAA,CAAA,IAAA,CAAA,kBAAA,EAAA;AACA,kBAAA,KAAA,EAAA,MAAA,CAAA,KADA;AAEA,kBAAA,WAAA,EAAA,MAAA,CAAA,WAFA;AAGA,kBAAA,IAAA,EAAA,EAAA,CAAA,IAAA,CAAA;AAHA,iBAAA,CALA;;AAAA;AAKA,gBAAA,EALA;AAUA,gBAAA,MAAA,CAAA,aAAA,GAAA,EAAA,CAAA,IAAA;AAVA;AAAA;;AAAA;AAAA;AAAA;AAYA,gBAAA,OAAA,CAAA,GAAA;;AAZA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAcA,KAlBA;AAmBA,IAAA,cAnBA,4BAmBA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBAEA,KAAA,CAAA,GAAA,CAAA,kBAAA,CAFA;;AAAA;AAEA,gBAAA,QAFA;AAGA,gBAAA,MAAA,CAAA,WAAA,GAAA,QAAA,CAAA,IAAA;AAHA,kDAIA,IAJA;;AAAA;AAAA;AAAA;AAMA,gBAAA,OAAA,CAAA,GAAA;;AANA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAQA,KA3BA;AA4BA,IAAA,gBA5BA,4BA4BA,UA5BA,EA4BA;AACA,WAAA,SAAA,GAAA,EAAA;AACA,WAAA,cAAA,GAAA,UAAA;AACA,KA/BA;AAgCA,IAAA,gBAhCA,4BAgCA,UAhCA,EAgCA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBAEA,KAAA,CAAA,MAAA,CAAA,sBAAA,UAAA,CAAA,GAAA,CAFA;;AAAA;AAGA,gBAAA,MAAA,CAAA,cAAA,GAAA,IAAA;;AACA,gBAAA,MAAA,CAAA,cAAA;;AAJA,kDAKA,IALA;;AAAA;AAAA;AAAA;AAOA,gBAAA,OAAA,CAAA,GAAA;;AAPA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AASA,KAzCA;AA0CA,IAAA,cA1CA,0BA0CA,UA1CA,EA0CA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBAEA,KAAA,CAAA,GAAA,CAAA,sBAAA,UAAA,CAAA,GAAA,EAAA;AACA,kBAAA,KAAA,EAAA,MAAA,CAAA,cAAA,CAAA,KADA;AAEA,kBAAA,WAAA,EAAA,MAAA,CAAA,cAAA,CAAA;AAFA,iBAAA,CAFA;;AAAA;AAMA,gBAAA,MAAA,CAAA,cAAA,GAAA,IAAA;;AACA,gBAAA,MAAA,CAAA,cAAA;;AAPA,kDAQA,IARA;;AAAA;AAAA;AAAA;AAUA,gBAAA,OAAA,CAAA,GAAA;;AAVA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAYA;AAtDA;AAtBA,CAAA","sourcesContent":["<template>\n<div class=\"admin\">\n  <h1>The Admin Page!</h1>\n  <div class=\"heading\">\n    <div class=\"circle\">1</div>\n    <h2>Add an Adventurer</h2>\n  </div>\n  <div class=\"add\">\n    <div class=\"form\">\n      <input v-model=\"title\" placeholder=\"Title\">\n      <textarea v-model=\"description\" placeholder=\"Adventurer Description\" rows=\"1\" cols=\"80\"></textarea>\n      <p></p>\n\n      <input type=\"file\" name=\"photo\" @change=\"fileChanged\">\n      <button @click=\"upload\">Upload</button>\n    </div>\n    <div class=\"upload\" v-if=\"addAdventurer\">\n      <h2>{{addAdventurer.title}}</h2>\n      <img :src=\"addAdventurer.path\" />\n    </div>\n  </div>\n  <div class=\"heading\">\n    <div class=\"circle\">2</div>\n    <h2>Edit/Delete an Adventurer</h2>\n  </div>\n  <div class=\"edit\">\n    <div class=\"form\">\n      <input v-model=\"findTitle\" placeholder=\"Search\">\n      <div class=\"suggestions\" v-if=\"suggestions.length > 0\">\n        <div class=\"suggestion\" v-for=\"s in suggestions\" :key=\"s.id\" @click=\"selectAdventurer(s)\">{{s.title}}\n        </div>\n      </div>\n    </div>\n    <div class=\"upload\" v-if=\"findAdventurer\">\n      <input v-model=\"findAdventurer.title\">\n      <p></p>\n      <img :src=\"findAdventurer.path\" />\n    </div>\n    <div class=\"actions\" v-if=\"findAdventurer\">\n      <button @click=\"deleteAdventurer(findAdventurer)\">Delete</button>\n      <button @click=\"editAdventurer(findAdventurer)\">Edit</button>\n      <textarea v-model=\"findAdventurer.description\" placeholder=\"Add Adventurer Description\" rows=\"8\" cols=\"80\"></textarea>\n    </div>\n  </div>\n</div>\n</template>\n\n<style scoped>\n.image h2 {\n  font-style: italic;\n  font-size: 1em;\n}\n\n.heading {\n  display: flex;\n  margin-bottom: 20px;\n  margin-top: 20px;\n}\n\n.heading h2 {\n  margin-top: 8px;\n  margin-left: 10px;\n}\n\n.add,\n.edit {\n  display: flex;\n}\n\n.circle {\n  border-radius: 50%;\n  width: 18px;\n  height: 18px;\n  padding: 8px;\n  background: #333;\n  color: #fff;\n  text-align: center\n}\n\n/* Form */\ninput,\ntextarea,\nselect,\nbutton {\n  font-family: 'Montserrat', sans-serif;\n  font-size: 1em;\n}\n\ntextarea {\n  margin-left: 10px;\n}\n\nbutton {\n  margin-bottom: 10px;\n}\n.form {\n  margin-right: 50px;\n}\n\n/* Uploaded images */\n.upload h2 {\n  margin: 0px;\n}\n\n.upload img {\n  max-width: 300px;\n}\n\n/* Suggestions */\n.suggestions {\n  width: 200px;\n  border: 1px solid #ccc;\n}\n\n.suggestion {\n  min-height: 20px;\n}\n\n.suggestion:hover {\n  background-color: #5BDEFF;\n  color: #fff;\n}\n</style>\n\n<script>\n  import axios from 'axios';\n  export default {\n    name: 'Admin',\n    data() {\n      return {\n        title: \"\",\n        description: \"\",\n        file: null,\n        addAdventurer: null,\n        adventurers: [],\n        findTitle: \"\",\n        findAdventurer: null,\n      }\n    },\n    computed: {\n      suggestions() {\n        let adventurers = this.adventurers.filter(adventurer => adventurer.title.toLowerCase().startsWith(this.findTitle.toLowerCase()));\n        return adventurers.sort((a, b) => a.title > b.title);\n      }\n    },\n    created() {\n      this.getAdventurers();\n    },\n    methods: {\n      fileChanged(event) {\n        this.file = event.target.files[0]\n      },\n      async upload() {\n        try {\n          const formData = new FormData();\n          formData.append('photo', this.file, this.file.name)\n          let r1 = await axios.post('/api/photos', formData);\n          let r2 = await axios.post('/api/adventurers', {\n            title: this.title,\n            description: this.description,\n            path: r1.data.path\n          });\n          this.addAdventurer = r2.data;\n        } catch (error) {\n          console.log(error);\n        }\n      },\n      async getAdventurers() {\n        try {\n          let response = await axios.get(\"/api/adventurers\");\n          this.adventurers = response.data;\n          return true;\n        } catch (error) {\n          console.log(error);\n        }\n      },\n      selectAdventurer(adventurer) {\n        this.findTitle = \"\";\n        this.findAdventurer = adventurer;\n      },\n      async deleteAdventurer(adventurer) {\n        try {\n          await axios.delete(\"/api/adventurers/\" + adventurer._id);\n          this.findAdventurer = null;\n          this.getAdventurers();\n          return true;\n        } catch (error) {\n          console.log(error);\n        }\n      },\n      async editAdventurer(adventurer) {\n        try {\n          await axios.put(\"/api/adventurers/\" + adventurer._id, {\n            title: this.findAdventurer.title,\n            description: this.findAdventurer.description,\n          });\n          this.findAdventurer = null;\n          this.getAdventurers();\n          return true;\n        } catch (error) {\n          console.log(error);\n        }\n      },\n    }\n  }\n</script>\n"],"sourceRoot":"src/views"}]}